{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**", ".observable/dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "docs:build": {
      "outputs": [".observable/dist/**"]
    },
    "docs:preview": {
      "cache": false,
      "persistent": true
    }
  }
}
